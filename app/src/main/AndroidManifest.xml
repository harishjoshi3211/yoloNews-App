<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newsappinternational">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".applicationclass"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".country"></activity>
        <activity android:name=".language" />
        <activity android:name=".briefnews" />
        <activity android:name=".shownews" />
        <activity android:name=".publications" />
        <activity android:name=".category" />
        <activity android:name=".showbycat" />
        <activity android:name=".headline" />
        <activity android:name=".register" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>